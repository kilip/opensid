<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Area" table="area">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="nama" type="string" column="nama" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="path" type="text" column="path" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enabled" type="bigint" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="refPolygon" type="integer" column="ref_polygon" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idCluster" type="bigint" column="id_cluster" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="desk" type="text" column="desk" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
