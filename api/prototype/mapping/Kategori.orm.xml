<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Kategori" table="kategori">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="kategori" type="string" column="kategori" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipe" type="integer" column="tipe" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="urut" type="smallint" column="urut" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="enabled" type="smallint" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="parrent" type="smallint" column="parrent" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
