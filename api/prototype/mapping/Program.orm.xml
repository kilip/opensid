<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Program" table="program">
    <id name="id" type="integer" length="11" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="nama" type="string" column="nama" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sasaran" type="smallint" column="sasaran" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ndesc" type="string" column="ndesc" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sdate" type="date" column="sdate" nullable="false"/>
    <field name="edate" type="date" column="edate" nullable="false"/>
    <field name="userid" type="integer" column="userid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>

      </options>
    </field>
    <field name="asaldana" type="string" column="asaldana" length="30" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
