<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Inbox" table="inbox">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="updatedindb" type="datetime" column="updatedindb" nullable="true"/>
    <field name="receivingdatetime" type="datetime" column="receivingdatetime" nullable="false">
      <options>

      </options>
    </field>
    <field name="text" type="text" column="text" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sendernumber" type="string" column="sendernumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>

      </options>
    </field>
    <field name="coding" type="string" column="coding" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Default_No_Compression</option>
      </options>
    </field>
    <field name="udh" type="text" column="udh" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smscnumber" type="string" column="smscnumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>

      </options>
    </field>
    <field name="class" type="bigint" column="class" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="textdecoded" type="text" column="textdecoded" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recipientid" type="text" column="recipientid" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="processed" type="string" column="processed" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
