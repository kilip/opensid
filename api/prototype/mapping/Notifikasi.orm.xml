      <generator strategy="IDENTITY"/>

    </id>
    <field name="kode" type="string" column="kode" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="judul" type="string" column="judul" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jenis" type="string" column="jenis" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isi" type="text" column="isi" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="server" type="string" column="server" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglBerikutnya" type="datetime" column="tgl_berikutnya" nullable="true"/>
