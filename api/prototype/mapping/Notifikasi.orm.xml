    <field name="updatedBy" type="bigint" column="updated_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frekuensi" type="smallint" column="frekuensi" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="aksi" type="string" column="aksi" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aktif" type="smallint" column="aktif" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
