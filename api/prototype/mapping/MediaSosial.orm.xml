<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\MediaSosial" table="media_sosial">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="gambar" type="text" column="gambar" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="link" type="text" column="link" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nama" type="string" column="nama" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipe" type="boolean" column="tipe" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="enabled" type="bigint" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
