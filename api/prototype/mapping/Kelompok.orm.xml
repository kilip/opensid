<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Kelompok" table="kelompok">
    <indexes>
      <index name="idx_37452_id_master" columns="id_master"/>
      <index name="idx_37452_id_ketua" columns="id_ketua"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_37452_kode" columns="kode"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>

    </id>
    <field name="idMaster" type="bigint" column="id_master" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idKetua" type="bigint" column="id_ketua" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nama" type="string" column="nama" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keterangan" type="string" column="keterangan" length="300" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kode" type="string" column="kode" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
