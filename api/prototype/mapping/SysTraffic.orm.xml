<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\SysTraffic" table="sys_traffic">
    <id name="tanggal" type="date" column="tanggal">
      <generator strategy="UUID"/>
    </id>
    <field name="ipaddress" type="text" column="ipaddress" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jumlah" type="bigint" column="jumlah" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
