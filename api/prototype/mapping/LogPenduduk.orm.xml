    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idPend" type="bigint" column="id_pend" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="kodePeristiwa" type="bigint" column="kode_peristiwa" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="meninggalDi" type="string" column="meninggal_di" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alamatTujuan" type="text" column="alamat_tujuan" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglLapor" type="datetime" column="tgl_lapor" nullable="false">
      <options>

      </options>
    </field>
    <field name="tglPeristiwa" type="datetime" column="tgl_peristiwa" nullable="true">
      <options>

      </options>
    </field>
    <field name="catatan" type="text" column="catatan" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noKk" type="decimal" column="no_kk" precision="16" scale="0" nullable="true"/>
    <field name="namaKk" type="string" column="nama_kk" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>

      </options>
    </field>
    <field name="createdBy" type="bigint" column="created_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="updatedBy" type="bigint" column="updated_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="refPindah" target-entity="RefPindah" fetch="LAZY">
      <join-columns>
        <join-column name="ref_pindah" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
