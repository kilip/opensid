<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="OpenSID\Prototype\Model\CaptchaCodes" table="captcha_codes">
        <!--
        <indexes>
            <index name="idx_37194_created" columns="created"/>
        </indexes>
        -->
        <id name="id" type="string" column="id" length="40"/>
        <id name="namespace" type="string" column="namespace" length="32"/>
        <field name="code" type="string" column="code" length="32" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="codeDisplay" type="string" column="code_display" length="32" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="created" type="integer" length="11" column="created" nullable="false"/>
        <field name="audioData" type="blob" column="audio_data" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
    </entity>
</doctrine-mapping>
