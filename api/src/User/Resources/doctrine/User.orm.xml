<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity
        name="OpenSID\User\Model\User"
        table="passport_user"
        repository-class="OpenSID\User\Repository\UserRepository"
    >
        <id name="id" type="string">
            <generator strategy="UUID"/>
        </id>
        <field name="username" length="100"/>
        <field name="password" nullable="true" length="100"/>
        <many-to-one field="group" target-entity="OpenSID\User\Contracts\GroupInterface"/>
        <field name="email" length="100"/>
        <field name="lastLogin" type="datetime" nullable="true"/>
        <field name="active" type="boolean"/>
        <field name="nama" nullable="true" length="50"/>
        <field name="company" nullable="true" length="100"/>
        <field name="phone" nullable="true" length="20"/>
        <field name="foto" nullable="true" length="100"/>
        <field name="session" length="40" nullable="true"/>
    </entity>
</doctrine-mapping>
