<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\SuratMasuk" table="surat_masuk">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomorUrut" type="smallint" column="nomor_urut" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tanggalPenerimaan" type="date" column="tanggal_penerimaan" nullable="false"/>
    <field name="nomorSurat" type="string" column="nomor_surat" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kodeSurat" type="string" column="kode_surat" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tanggalSurat" type="date" column="tanggal_surat" nullable="false"/>
    <field name="pengirim" type="string" column="pengirim" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isiSingkat" type="string" column="isi_singkat" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isiDisposisi" type="string" column="isi_disposisi" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="berkasScan" type="string" column="berkas_scan" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
