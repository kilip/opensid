<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Sentitems" table="sentitems">
    <indexes>
      <index name="sentitems_dest" columns="DestinationNumber"/>
      <index name="sentitems_sender" columns="SenderID"/>
      <index name="sentitems_date" columns="DeliveryDateTime"/>
      <index name="sentitems_tpmr" columns="TPMR"/>
    </indexes>
    <id name="id" type="integer" column="ID"/>
    <id name="sequenceposition" type="integer" column="SequencePosition"/>
    <field name="updatedindb" type="datetime" column="UpdatedInDB" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="insertintodb" type="datetime" column="InsertIntoDB" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="sendingdatetime" type="datetime" column="SendingDateTime" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="deliverydatetime" type="datetime" column="DeliveryDateTime" nullable="true"/>
    <field name="text" type="text" column="Text" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="destinationnumber" type="string" column="DestinationNumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="coding" type="string" column="Coding" length="0" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Default_No_Compression</option>
      </options>
    </field>
    <field name="udh" type="text" column="UDH" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smscnumber" type="string" column="SMSCNumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="class" type="integer" column="Class" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="textdecoded" type="text" column="TextDecoded" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="senderid" type="string" column="SenderID" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="Status" length="0" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">SendingOK</option>
      </options>
    </field>
    <field name="statuserror" type="integer" column="StatusError" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="tpmr" type="integer" column="TPMR" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="relativevalidity" type="integer" column="RelativeValidity" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="creatorid" type="text" column="CreatorID" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
