<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\KeuanganTaPerangkat" table="keuangan_ta_perangkat">
    <indexes>
      <index name="id_keuangan_ta_perangkat_master_fk" columns="id_keuangan_master"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tahun" type="string" column="Tahun" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kdDesa" type="string" column="Kd_Desa" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kdJabatan" type="string" column="Kd_Jabatan" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noId" type="string" column="No_ID" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaPerangkat" type="string" column="Nama_Perangkat" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alamatPerangkat" type="string" column="Alamat_Perangkat" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomorHp" type="string" column="Nomor_HP" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rekBank" type="string" column="Rek_Bank" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaBank" type="string" column="Nama_Bank" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
