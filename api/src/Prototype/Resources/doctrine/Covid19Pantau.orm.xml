<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Covid19Pantau" table="covid19_pantau">
    <indexes>
      <index name="fk_pantau_pemudik" columns="id_pemudik"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tanggalJam" type="datetime" column="tanggal_jam" nullable="true"/>
    <field name="suhuTubuh" type="decimal" column="suhu_tubuh" precision="4" scale="2" nullable="true"/>
    <field name="batuk" type="string" column="batuk" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="flu" type="string" column="flu" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sesakNafas" type="string" column="sesak_nafas" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keluhanLain" type="string" column="keluhan_lain" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusCovid" type="string" column="status_covid" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idPemudik" target-entity="Covid19Pemudik" fetch="LAZY">
      <join-columns>
        <join-column name="id_pemudik" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
