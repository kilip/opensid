<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Menu" table="menu">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nama" type="string" column="nama" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="link" type="string" column="link" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipe" type="integer" column="tipe" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="parrent" type="integer" column="parrent" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="linkTipe" type="tinyint" column="link_tipe" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enabled" type="integer" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="urut" type="integer" column="urut" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
