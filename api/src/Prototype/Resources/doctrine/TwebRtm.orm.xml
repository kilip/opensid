<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\TwebRtm" table="tweb_rtm">
    <unique-constraints>
      <unique-constraint name="no_kk" columns="no_kk"/>
      <unique-constraint name="no_kk_2" columns="no_kk"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nikKepala" type="integer" column="nik_kepala" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="noKk" type="string" column="no_kk" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglDaftar" type="datetime" column="tgl_daftar" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="kelasSosial" type="integer" column="kelas_sosial" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
