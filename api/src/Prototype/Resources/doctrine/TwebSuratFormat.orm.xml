<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\TwebSuratFormat" table="tweb_surat_format">
    <unique-constraints>
      <unique-constraint name="url_surat" columns="url_surat"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nama" type="string" column="nama" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urlSurat" type="string" column="url_surat" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kodeSurat" type="string" column="kode_surat" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lampiran" type="string" column="lampiran" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kunci" type="tinyint" column="kunci" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="favorit" type="tinyint" column="favorit" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="jenis" type="tinyint" column="jenis" nullable="false">
      <options>
        <option name="default">2</option>
      </options>
    </field>
    <field name="mandiri" type="tinyint" column="mandiri" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="masaBerlaku" type="integer" column="masa_berlaku" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="satuanMasaBerlaku" type="string" column="satuan_masa_berlaku" length="15" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">M</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
