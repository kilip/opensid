<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\User" table="user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idGrup" type="integer" column="id_grup" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="active" type="tinyint" column="active" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nama" type="string" column="nama" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="company" type="string" column="company" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="session" type="string" column="session" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
