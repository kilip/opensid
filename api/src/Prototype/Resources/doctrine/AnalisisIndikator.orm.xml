<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\AnalisisIndikator" table="analisis_indikator">
    <indexes>
      <index name="id_kategori" columns="id_kategori"/>
      <index name="id_master" columns="id_master,id_tipe"/>
      <index name="id_tipe" columns="id_tipe"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idMaster" type="integer" column="id_master" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomor" type="integer" column="nomor" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pertanyaan" type="string" column="pertanyaan" length="400" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idTipe" type="tinyint" column="id_tipe" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="bobot" type="tinyint" column="bobot" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="actAnalisis" type="tinyint" column="act_analisis" nullable="false">
      <options>
        <option name="default">2</option>
      </options>
    </field>
    <field name="idKategori" type="integer" column="id_kategori" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isPublik" type="tinyint" column="is_publik" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isTeks" type="tinyint" column="is_teks" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
