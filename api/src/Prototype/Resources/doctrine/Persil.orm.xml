<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Persil" table="persil">
    <indexes>
      <index name="nomor_nomor_urut_bidang" columns="nomor,nomor_urut_bidang"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomor" type="string" column="nomor" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomorUrutBidang" type="tinyint" column="nomor_urut_bidang" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="kelas" type="integer" column="kelas" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="luasPersil" type="decimal" column="luas_persil" precision="7" scale="0" nullable="true"/>
    <field name="idWilayah" type="integer" column="id_wilayah" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lokasi" type="text" column="lokasi" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="path" type="text" column="path" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cdesaAwal" type="integer" column="cdesa_awal" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
