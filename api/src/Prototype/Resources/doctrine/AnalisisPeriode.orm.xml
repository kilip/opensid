<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\AnalisisPeriode" table="analisis_periode">
    <indexes>
      <index name="id_master" columns="id_master"/>
      <index name="id_state" columns="id_state"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idMaster" type="integer" column="id_master" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nama" type="string" column="nama" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idState" type="tinyint" column="id_state" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="aktif" type="tinyint" column="aktif" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="keterangan" type="string" column="keterangan" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tahunPelaksanaan" type="date" column="tahun_pelaksanaan" nullable="false"/>
  </entity>
</doctrine-mapping>
