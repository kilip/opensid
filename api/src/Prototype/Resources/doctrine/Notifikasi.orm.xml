<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Notifikasi" table="notifikasi">
    <unique-constraints>
      <unique-constraint name="kode" columns="kode"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kode" type="string" column="kode" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="judul" type="string" column="judul" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jenis" type="string" column="jenis" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isi" type="text" column="isi" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="server" type="string" column="server" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglBerikutnya" type="datetime" column="tgl_berikutnya" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedBy" type="integer" column="updated_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frekuensi" type="smallint" column="frekuensi" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="aksi" type="string" column="aksi" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aktif" type="tinyint" column="aktif" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
