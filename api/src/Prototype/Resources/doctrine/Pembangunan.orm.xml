<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Pembangunan" table="pembangunan">
    <indexes>
      <index name="id_lokasi" columns="id_lokasi"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idLokasi" type="integer" column="id_lokasi" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sumberDana" type="string" column="sumber_dana" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="judul" type="string" column="judul" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keterangan" type="string" column="keterangan" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lokasi" type="string" column="lokasi" length="225" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lat" type="string" column="lat" length="225" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lng" type="string" column="lng" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="volume" type="string" column="volume" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tahunAnggaran" type="date" column="tahun_anggaran" nullable="true"/>
    <field name="pelaksanaKegiatan" type="string" column="pelaksana_kegiatan" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="tinyint" column="status" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="foto" type="string" column="foto" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anggaran" type="integer" column="anggaran" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
