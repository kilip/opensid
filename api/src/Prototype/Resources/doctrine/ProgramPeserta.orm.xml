<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\ProgramPeserta" table="program_peserta">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="peserta" type="string" column="peserta" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="programId" type="integer" column="program_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="noIdKartu" type="string" column="no_id_kartu" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuNik" type="string" column="kartu_nik" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuNama" type="string" column="kartu_nama" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuTempatLahir" type="string" column="kartu_tempat_lahir" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuTanggalLahir" type="date" column="kartu_tanggal_lahir" nullable="true"/>
    <field name="kartuAlamat" type="string" column="kartu_alamat" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuPeserta" type="string" column="kartu_peserta" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kartuIdPend" type="integer" column="kartu_id_pend" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
