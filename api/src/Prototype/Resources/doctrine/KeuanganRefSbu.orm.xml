    <field name="kodeSbu" type="string" column="Kode_SBU" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nourutSbu" type="string" column="NoUrut_SBU" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaSbu" type="string" column="Nama_SBU" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nilai" type="string" column="Nilai" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="satuan" type="string" column="Satuan" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
