<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\SettingAplikasi" table="setting_aplikasi">
    <unique-constraints>
      <unique-constraint name="key" columns="key"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="key" type="string" column="key" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keterangan" type="string" column="keterangan" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jenis" type="string" column="jenis" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kategori" type="string" column="kategori" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
