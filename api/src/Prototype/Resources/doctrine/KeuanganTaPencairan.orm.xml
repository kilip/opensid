<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\KeuanganTaPencairan" table="keuangan_ta_pencairan">
    <indexes>
      <index name="id_keuangan_ta_pencairan_master_fk" columns="id_keuangan_master"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tahun" type="string" column="Tahun" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noCek" type="string" column="No_Cek" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noSpp" type="string" column="No_SPP" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglCek" type="string" column="Tgl_Cek" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kdDesa" type="string" column="Kd_Desa" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keterangan" type="string" column="Keterangan" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jumlah" type="string" column="Jumlah" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="potongan" type="string" column="Potongan" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kdbayar" type="string" column="KdBayar" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idBank" type="string" column="ID_Bank" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kunci" type="string" column="Kunci" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
