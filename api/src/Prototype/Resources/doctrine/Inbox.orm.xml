<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Inbox" table="inbox">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="updatedindb" type="datetime" column="UpdatedInDB" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="receivingdatetime" type="datetime" column="ReceivingDateTime" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="text" type="text" column="Text" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sendernumber" type="string" column="SenderNumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="coding" type="string" column="Coding" length="0" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Default_No_Compression</option>
      </options>
    </field>
    <field name="udh" type="text" column="UDH" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smscnumber" type="string" column="SMSCNumber" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="class" type="integer" column="Class" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="textdecoded" type="text" column="TextDecoded" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recipientid" type="text" column="RecipientID" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="processed" type="string" column="Processed" length="0" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
