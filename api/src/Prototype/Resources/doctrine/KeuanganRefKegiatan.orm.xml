<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\KeuanganRefKegiatan" table="keuangan_ref_kegiatan">
    <indexes>
      <index name="id_keuangan_ref_kegiatan_master_fk" columns="id_keuangan_master"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kdBid" type="string" column="Kd_Bid" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idKeg" type="string" column="ID_Keg" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaKegiatan" type="string" column="Nama_Kegiatan" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jnsKegiatan" type="tinyint" column="Jns_Kegiatan" nullable="true"/>
    <field name="kdSub" type="string" column="Kd_Sub" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
