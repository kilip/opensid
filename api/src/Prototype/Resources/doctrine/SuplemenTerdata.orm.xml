<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\SuplemenTerdata" table="suplemen_terdata">
    <indexes>
      <index name="id_suplemen" columns="id_suplemen"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idTerdata" type="string" column="id_terdata" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sasaran" type="tinyint" column="sasaran" nullable="true"/>
    <field name="keterangan" type="string" column="keterangan" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idSuplemen" target-entity="Suplemen" fetch="LAZY">
      <join-columns>
        <join-column name="id_suplemen" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
