    <field name="kdRincian" type="string" column="Kd_Rincian" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="norekBank" type="string" column="NoRek_Bank" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaBank" type="string" column="Nama_Bank" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kantorCabang" type="string" column="Kantor_Cabang" length="100" nullable="true">
      <options>
        <option name="fixed"/>
