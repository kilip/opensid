    <field name="noIdentitas" type="string" column="No_Identitas" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noTelepon" type="string" column="No_Telepon" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idBank" type="string" column="ID_Bank" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
