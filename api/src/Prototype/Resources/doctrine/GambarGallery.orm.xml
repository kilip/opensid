<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\GambarGallery" table="gambar_gallery">
    <indexes>
      <index name="parrent" columns="parrent"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="parrent" type="integer" column="parrent" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="gambar" type="string" column="gambar" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nama" type="string" column="nama" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enabled" type="integer" column="enabled" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="tglUpload" type="datetime" column="tgl_upload" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="tipe" type="integer" column="tipe" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="slider" type="tinyint" column="slider" nullable="true"/>
    <field name="urut" type="integer" column="urut" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
