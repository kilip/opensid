<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\Komentar" table="komentar">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idArtikel" type="integer" column="id_artikel" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="owner" type="string" column="owner" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjek" type="text" column="subjek" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="komentar" type="text" column="komentar" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tglUpload" type="datetime" column="tgl_upload" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="status" type="tinyint" column="status" nullable="true"/>
    <field name="tipe" type="tinyint" column="tipe" nullable="true"/>
    <field name="noHp" type="string" column="no_hp" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="isArchived" type="tinyint" column="is_archived" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
