<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\SettingModul" table="setting_modul">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="modul" type="string" column="modul" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="url" type="string" column="url" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aktif" type="tinyint" column="aktif" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ikon" type="string" column="ikon" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urut" type="tinyint" column="urut" nullable="false"/>
    <field name="level" type="tinyint" column="level" nullable="false">
      <options>
        <option name="default">2</option>
      </options>
    </field>
    <field name="hidden" type="tinyint" column="hidden" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ikonKecil" type="string" column="ikon_kecil" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parent" type="integer" column="parent" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
