<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\KeuanganRefBidang" table="keuangan_ref_bidang">
    <indexes>
      <index name="id_keuangan_ref_bidang_master_fk" columns="id_keuangan_master"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kdBid" type="string" column="Kd_Bid" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namaBidang" type="string" column="Nama_Bidang" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idKeuanganMaster" target-entity="KeuanganMaster" fetch="LAZY">
      <join-columns>
        <join-column name="id_keuangan_master" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
