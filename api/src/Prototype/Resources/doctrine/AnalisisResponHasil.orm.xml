<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OpenSID\Prototype\Model\AnalisisResponHasil" table="analisis_respon_hasil">
    <unique-constraints>
      <unique-constraint name="id_master" columns="id_master,id_periode,id_subjek"/>
    </unique-constraints>
    <id name="uuid" type="string" column="uuid" length="36">
      <generator strategy="UUID"/>
    </id>
    <field name="idMaster" type="tinyint" column="id_master" nullable="false"/>
    <field name="idPeriode" type="tinyint" column="id_periode" nullable="false"/>
    <field name="idSubjek" type="integer" column="id_subjek" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="akumulasi" type="float" column="akumulasi" precision="8" scale="3" nullable="false"/>
    <field name="tglUpdate" type="datetime" column="tgl_update" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
